<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>{% block title %}GLEZAND{% endblock %}</title>
        {% block stylesheets %}
            <link rel="icon" type="image/x-icon" href="{{ asset('glezand.svg') }}" />
            <link rel="stylesheet" href="{{ asset('build/main.css') }}" />
        {% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}
            <script src="{{ asset('build/vendor.js') }}"></script>
            <script src="{{ asset('build/main.js') }}"></script>
        {% endblock %}
    </body>
</html>
